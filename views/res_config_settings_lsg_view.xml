<odoo>
    <record id="view_canje_settings_lsg_tax" model="ir.ui.view">
        <field name="name">canje.settings.lsg.tax</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="grain_canje_triangular.view_canje_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='grain_lpg_tax_id']" position="after">
                <field name="grain_lsg_tax_id"/>
            </xpath>
        </field>
    </record>
</odoo>
